<?xml version="1.0" encoding="UTF-8"?>
<schema
    xmlns="http://www.concrete5.org/doctrine-xml/0.5"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 https://concretecms.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>

    <table name="btCTTopTranslators">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="numTranslators" type="integer">
            <unsigned/>
        </field>
        <field name="limitToLocale" type="string" size="12">
            <notnull/>
        </field>
        <field name="allTranslations" type="boolean">
            <notnull/>
        </field>
    </table>

</schema>
